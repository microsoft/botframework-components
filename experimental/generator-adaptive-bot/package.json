{
  "name": "generator-adaptive-bot",
  "version": "1.0.0-preview.5",
  "description": "Yeoman generator for creating an Adaptive Bot application built on top of the Microsoft Bot Framework SDK and runtime.",
  "files": [
    "generators"
  ],
  "keywords": [
    "microsoft",
    "bot",
    "framework",
    "adaptive",
    "yeoman-generator"
  ],
  "dependencies": {
    "uuid": "^8.3.2",
    "yeoman-generator": "^4.12.0"
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.7",
    "@types/uuid": "^8.3.0",
    "@types/yeoman-generator": "^4.11.3",
    "copyfiles": "^2.4.1",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.3",
    "typescript": "^4.1.3"
  },
  "author": "Bot Framework",
  "license": "ISC",
  "scripts": {
    "build": "npm-run-all build:tsc build:copy",
    "build:copy": "npm-run-all -p build:copy:dotnet",
    "build:copy:dotnet": "shx cp -R src/dotnet/templates generators/dotnet/templates",
    "build:tsc": "tsc -b",
    "clean": "shx rm -rf generators tsconfig.tsbuildinfo",
    "prepare": "npm run build"
  }
}
