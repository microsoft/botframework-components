{
  "MicrosoftAppPassword": "",
  "MicrosoftAppId": "",
  "defaultLocale": "en-US",
  "runtimeSettings": {
    "features": {
      "removeRecipientMentions": false,
      "showTyping": false,
      "useInspection": false,
      "setSpeak": true,
      "traceTranscript": false
    },
    "telemetry": {
      "instrumentationKey": "",
      "logPersonalInformation": false,
      "logActivities": true
    },
    "plugins": [
      {
        "name": "Microsoft.Bot.Components.Calendar",
        "settingsPrefix": "Microsoft.Bot.Components.Calendar"
      },
      {
        "name": "Microsoft.Bot.Components.Graph",
        "settingsPrefix": "Microsoft.Bot.Components.Graph"
      }
    ],
    "skills": {
      "allowedCallers": []
    },
    "adapters": []
  },
  "luis": {
    "name": "Assistant",
    "authoringKey": "",
    "endpointKey": "",
    "authoringRegion": "westus",
    "defaultLanguage": "en-us",
    "environment": "composer",
    "endpoint": "",
    "authoringEndpoint": ""
  },
  "publishTargets": [],
  "qna": {
    "knowledgebaseid": "",
    "endpointkey": "",
    "hostname": ""
  },
  "runtime": {
    "customRuntime": true,
    "path": "../",
    "command": "dotnet run --project <%= botName %>.csproj",
    "key": "csharp-azurewebapp-v2"
  },
  "downsampling": {
    "maxImbalanceRatio": 10,
    "maxUtteranceAllowed": 15000
  },
  "defaultLanguage": "en-us",
  "languages": ["en-us"],
  "oauthConnectionName": "Outlook",
  "defaultValue": {
    "duration": 30
  }
