{
  "$schema": "https://schemas.botframework.com/schemas/component/v1.0/component.schema",
  "$role": "implements(Microsoft.IDialog)",
  "type": "object",
  "title": "Microsoft Graph - Get Profile of A User",
  "description": "Get a user profile using the Microsoft Graph API.",
  "required": [
    "token",
    "userId"
  ],
  "properties": {
    "id": {
      "type": "string",
      "title": "Id",
      "description": "Optional id for the dialog"
    },
    "resultProperty": {
      "$ref": "schema:#/definitions/stringExpression",
      "title": "Result property",
      "description": "Named state location to store result.",
      "examples": []
    },
    "token": {
      "$ref": "schema:#/definitions/valueExpression",
      "title": "Token",
      "description": "Microsoft Graph API authentication token.",
      "examples": [
        "user.token.token"
      ]
    },
    "fieldsToSelect": {
      "$ref": "schema:#/definitions/stringExpression",
      "title": "Fields To Select",
      "description": "The fields of from the graph API to select. By default, if not provided only the ID will be returned for the object.",
      "examples": [
        "dialog.fieldsToSelect"
      ]
    },
    "userId": {
      "$ref": "schema:#/definitions/stringExpression",
      "title": "User ID",
      "description": "The ID of the user to get the profile of",
      "examples": [
        "dialog.user.id"
      ]
    }
  }
}
