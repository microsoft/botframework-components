{
  "$schema": "https://raw.githubusercontent.com/microsoft/BotFramework-Composer/stable/Composer/packages/server/schemas/sdk.schema",
  "$kind": "Microsoft.Test.Script",
  "properties": [
    "settings.mock"
  ],
  "userTokens": [
    "usertoken.mock"
  ],
  "httpRequestMocks": [
    "getmeetings.mock",
  ],
  "dialog": "calendarskill",
  "script": [
    {
      "$kind": "Microsoft.Test.UserConversationUpdate",
      "membersAdded": [
        "User"
      ]
    },
    {
      "$kind": "Microsoft.Test.AssertReplyOneOf",
      "text": [
        "Hi, what can I do for you?"
      ]
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "connect to my meeting"
    },
    {
      "$kind": "Microsoft.Test.AssertReplyOneOf",
      "text": [
        "What is the time or subject of the meeting?"
      ]
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "test"
    },
    {
      "$kind": "Microsoft.Test.AssertReplyActivity",
      "assertions": [
        "attachments[0].contentType == 'application/vnd.microsoft.card.oauth'"
      ]
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "000000"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "Here's the number I'm about to call: 123456. Please be aware that charges may apply. Do you want to continue? (1) Yes or (2) No"
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "yes"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "OK, I'll call into the meeting."
    },
    {
      "$kind": "Microsoft.Test.AssertReplyActivity",
      "assertions": [
        "type == 'event'",
        "value.MeetingUri == 'testurl'",
        "value.TelephoneUri == '123456'"
      ]
    }
  ]
}
