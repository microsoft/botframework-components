{
  "$schema": "https://raw.githubusercontent.com/microsoft/BotFramework-Composer/stable/Composer/packages/server/schemas/sdk.schema",
  "$kind": "Microsoft.Test.Script",
  "properties": [
    "settings.mock"
  ],
  "userTokens": [
    "usertoken.mock"
  ],
  "httpRequestMocks": [
    "getcontacts.mock",
    "getpeople.mock",
    "getphoto.mock",
    "createevent.mock",
    "getme.mock",
    "getschedule_person_conflictinvitee.mock"
  ],
  "dialog": "calendarskill",
  "script": [
    {
      "$kind": "Microsoft.Test.UserConversationUpdate",
      "membersAdded": [
        "User"
      ]
    },
    {
      "$kind": "Microsoft.Test.AssertReplyOneOf",
      "text": [
        "Hi, what can I do for you?"
      ]
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "is test available at 3pm on 2020-07-02?"
    },
    {
      "$kind": "Microsoft.Test.AssertReplyActivity",
      "assertions": [
        "attachments[0].contentType == 'application/vnd.microsoft.card.oauth'"
      ]
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "000000"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "It looks like test name isn't available on 2020-07-02 at 15:00:00."
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "Do you want me to find a time you and test name are both available? (1) Yes or (2) No"
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "yes"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "OK, you and test name are both available from 15:05:00 to 18:00:00 on 2020-07-02."
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "Do you want to schedule a new meeting? (1) Yes or (2) No"
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "yes"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "What's the subject of the meeting?"
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "lunch"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "Can you provide a short description for the meeting?"
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "just for lunch"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "For how long?"
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "30 min"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "Where will the event take place?"
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "london"
    },
    {
      "$kind": "Microsoft.Test.AssertReplyActivity",
      "assertions": [
        "count(attachments) == 1"
      ]
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "Do you want to create an event using this info? (1) Yes or (2) No"
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "yes"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "Alright, your meeting is confirmed."
    }
  ]
}
