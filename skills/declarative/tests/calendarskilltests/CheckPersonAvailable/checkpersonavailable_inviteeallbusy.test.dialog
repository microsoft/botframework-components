{
  "$schema": "https://raw.githubusercontent.com/microsoft/BotFramework-Composer/stable/Composer/packages/server/schemas/sdk.schema",
  "$kind": "Microsoft.Test.Script",
  "propertyMocks": [
    "settings.mock"
  ],
  "userTokenMocks": [
    "usertoken.mock"
  ],
  "httpRequestMocks": [
    "getcontacts.mock",
    "getpeople.mock",
    "getphoto.mock",
    "createevent.mock",
    "getme.mock",
    "getschedule_person_inviteeallbusy.mock"
  ],
  "dialog": "calendarskill",
  "script": [
    {
      "$kind": "Microsoft.Test.UserConversationUpdate",
      "membersAdded": [
        "User"
      ]
    },
    {
      "$kind": "Microsoft.Test.AssertReplyOneOf",
      "text": [
        "Hi, what can I do for you?"
      ]
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "is test available at 3pm on 2020-07-02?"
    },
    {
      "$kind": "Microsoft.Test.AssertReplyActivity",
      "assertions": [
        "attachments[0].contentType == 'application/vnd.microsoft.card.oauth'"
      ]
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "000000"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "It looks like test name isn't available on 2020-07-02 at 15:00:00."
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "Do you want me to find a time you and test name are both available? (1) Yes or (2) No"
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "yes"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "Sorry, I can't find a time when you and test name are both available."
    }
  ]
}
