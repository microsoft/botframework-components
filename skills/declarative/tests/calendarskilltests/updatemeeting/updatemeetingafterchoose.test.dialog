{
  "$schema": "https://raw.githubusercontent.com/microsoft/BotFramework-Composer/stable/Composer/packages/server/schemas/sdk.schema",
  "$kind": "Microsoft.Test.Script",
  "propertyMocks": [
    "settings.mock"
  ],
  "userTokenMocks": [
    "usertoken.mock"
  ],
  "httpRequestMocks": [
    "getmultiplemeetings.mock",
    "updatemeeting.mock"
  ],
  "dialog": "calendarskill",
  "script": [
    {
      "$kind": "Microsoft.Test.UserConversationUpdate",
      "membersAdded": [
        "User"
      ]
    },
    {
      "$kind": "Microsoft.Test.AssertReplyOneOf",
      "text": [
        "Hi, what can I do for you?"
      ]
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "update my meeting"
    },
    {
      "$kind": "Microsoft.Test.AssertReplyOneOf",
      "text": [
        "What is the time or subject of the meeting?"
      ]
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "2pm"
    },
    {
      "$kind": "Microsoft.Test.AssertReplyActivity",
      "assertions": [
        "attachments[0].contentType == 'application/vnd.microsoft.card.oauth'"
      ]
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "000000"
    },
    {
      "$kind": "Microsoft.Test.AssertReplyActivity",
      "assertions": [
        "text == 'I found more than one event that might match. Please select the one you want. (1) test subject, (2) test subject, or (3) test subject'",
        "count(attachments) == 1"
      ]
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "1"
    },
    {
      "$kind": "Microsoft.Test.AssertReplyOneOf",
      "text": [
        "What's the date for your event?"
      ]
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "today"
    },
    {
      "$kind": "Microsoft.Test.AssertReplyOneOf",
      "text": [
        "What's the start time?",
        "What time does it start?"
      ]
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "3pm"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "Are you confirmed to update the meeting \"test subject\" (1) Yes or (2) No"
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "yes"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "Updated meeting \"test subject\""
    }
  ]
}
