{
  "$schema": "https://raw.githubusercontent.com/microsoft/BotFramework-Composer/stable/Composer/packages/server/schemas/sdk.schema",
  "$kind": "Microsoft.Test.Script",
  "propertyMocks": [
    "settings.mock"
  ],
  "userTokenMocks": [
    "usertoken.mock"
  ],
  "httpRequestMocks": [
    "getlist.mock",
    "gettaskpages.mock"
  ],
  "dialog": "ToDoSkill",
  "script": [
    {
      "$kind": "Microsoft.Test.UserConversationUpdate",
      "membersAdded": [
        "User"
      ]
    },
    {
      "$kind": "Microsoft.Test.AssertReplyOneOf",
      "text": [
        "Hello, I'm the Todo sample bot. What can I do for you today?"
      ]
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "show my tasks"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "What list would you like to see? (1) Shopping, (2) Grocery, or (3) To Do"
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "todo"
    },
    {
      "$kind": "Microsoft.Test.AssertReplyActivity",
      "assertions": [
        "attachments[0].contentType == 'application/vnd.microsoft.card.oauth'"
      ]
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "000000"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "You have 5 items on your To Do list. Here are the ones in page 1:"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "Let me know if you want to add an item or mark one as complete."
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "show next"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "You have 5 items on your To Do list. Here are the ones in page 2:"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "Let me know if you want to add an item or mark one as complete."
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "show next"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "You have 5 items on your To Do list. Here are the ones in page 3:"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "Let me know if you want to add an item or mark one as complete."
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "show previous"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "You have 5 items on your To Do list. Here are the ones in page 2:"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "Let me know if you want to add an item or mark one as complete."
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "show previous"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "You have 5 items on your To Do list. Here are the ones in page 1:"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "Let me know if you want to add an item or mark one as complete."
    }
  ]
}
