{
  "$schema": "https://raw.githubusercontent.com/microsoft/BotFramework-Composer/stable/Composer/packages/server/schemas/sdk.schema",
  "$kind": "Microsoft.Test.Script",
  "propertyMocks": [
    "settings.mock"
  ],
  "userTokenMocks": [
    "usertoken.mock"
  ],
  "httpRequestMocks": [
    "getlist.mock",
    "gettask.mock"
  ],
  "skillClaim": "skillclaim.mock",
  "dialog": "ToDoSkill",
  "script": [
    {
      "$kind": "Microsoft.Test.UserActivity",
      "activity": {
        "type": "event",
        "name": "ShowToDo",
        "locale": "en-US",
        "value": {
          "listType": "todo"
        }
      }
    },
    {
      "$kind": "Microsoft.Test.AssertReplyActivity",
      "assertions": [
        "attachments[0].contentType == 'application/vnd.microsoft.card.oauth'"
      ]
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "000000"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "The most recent item on To Do list is:"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "Let me know if you want to add an item or mark one as complete."
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "done"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "I'm here if you need anything else."
    },
    {
      "$kind": "Microsoft.Test.AssertReplyActivity",
      "assertions": [
        "type == 'endOfConversation'",
        "value.actionSuccess == true"
      ]
    }
  ]
}
