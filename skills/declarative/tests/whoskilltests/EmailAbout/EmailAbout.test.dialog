{
    "$schema": "https://raw.githubusercontent.com/microsoft/BotFramework-Composer/stable/Composer/packages/server/schemas/sdk.schema",
    "$kind":"Microsoft.Test.Script",
    "propertyMocks": [
		"settings.mock"
	],
	"userTokenMocks": [
		"usertoken.mock"
	],
	"httpRequestMocks": [
		"GetMessages.mock",
		"Me.mock",
		"GetUserByEmail_allan.mock",
		"GetUserByEmail_me.mock",
		"GetPhoto.mock"
	],
    "dialog":"whoskill",
    "script":[
        {
            "$kind":"Microsoft.Test.UserSays",
            "text":"Who emailed me recently?"
        },
        {
            "$kind":"Microsoft.Test.AssertReplyActivity",
            "assertions":[
                "attachments[0].contentType == 'application/vnd.microsoft.card.oauth'"
            ]
        },
        {
            "$kind":"Microsoft.Test.UserSays",
            "text":"000000"
        },
        {
            "$kind":"Microsoft.Test.AssertReply",
            "text":"You exchanged emails with at least 1 people."
        },
        {
            "$kind":"Microsoft.Test.AssertReplyActivity",
            "assertions":[
                "attachments[0].contentType == 'application/vnd.microsoft.card.adaptive'"
            ]
        }
    ]
}