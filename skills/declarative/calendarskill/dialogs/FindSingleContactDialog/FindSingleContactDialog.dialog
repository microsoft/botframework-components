{
  "$kind": "Microsoft.AdaptiveDialog",
  "$designer": {
    "id": "0bGhKY",
    "name": "FindSingleContactDialog"
  },
  "autoEndDialog": "true",
  "defaultResultProperty": "dialog.result",
  "triggers": [
    {
      "$kind": "Microsoft.OnBeginDialog",
      "$designer": {
        "name": "BeginDialog",
        "id": "Azbt3O"
      },
      "actions": [
        {
          "$kind": "Microsoft.BeginDialog",
          "$designer": {
            "id": "RoIRsm"
          },
          "dialog": "GetContactRequest",
          "resultProperty": "dialog.attendeeResult",
          "options": {
            "attendee": "=dialog.CurrentAttendee"
          }
        },
        {
          "$kind": "Microsoft.TraceActivity",
          "$designer": {
            "id": "e38q0j"
          }
        },
        {
          "$kind": "Microsoft.IfCondition",
          "$designer": {
            "id": "SPWVAY"
          },
          "condition": "dialog.attendeeResult.contactsList !=null && count(dialog.attendeeResult.contactsList) > 0",
          "elseActions": [
            {
              "$kind": "Microsoft.TextInput",
              "$designer": {
                "id": "d1fnSD"
              },
              "maxTurnCount": 3,
              "alwaysPrompt": false,
              "allowInterruptions": false,
              "prompt": "${bfdprompt-d1fnSD()}",
              "property": "dialog.NewAttendee"
            },
            {
              "$kind": "Microsoft.SetProperty",
              "$designer": {
                "id": "Fy6y8k"
              },
              "property": "dialog.options['dialog.CurrentAttendee']",
              "value": "dialog.NewAttendee"
            },
            {
              "$kind": "Microsoft.RepeatDialog",
              "$designer": {
                "id": "kiQTMV"
              }
            }
          ]
        },
        {
          "$kind": "Microsoft.SetProperty",
          "$designer": {
            "id": "FRMDtx"
          },
          "property": "turn.ResolvedAttendee",
          "value": "=dialog.attendeeResult.contactsList[0].mail"
        },
        {
          "$kind": "Microsoft.EndDialog",
          "$designer": {
            "id": "JCNE2_"
          },
          "value": "=turn.ResolvedAttendee"
        }
      ]
    }
  ],
  "generator": "FindSingleContactDialog.lg"
}
