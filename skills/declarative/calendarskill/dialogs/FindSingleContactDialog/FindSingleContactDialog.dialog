          "$kind": "Microsoft.BeginDialog",
          "$designer": {
            "id": "RoIRsm"
          },
          "dialog": "GetContactRequest",
          "resultProperty": "dialog.attendeeResult",
          "options": {
            "attendee": "=dialog.attendee"
          }
        },
        {
          "$kind": "Microsoft.TraceActivity",
          "$designer": {
            "id": "e38q0j"
          }
        },
        {
          "$kind": "Microsoft.IfCondition",
          "$designer": {
            "id": "SPWVAY"
          },
          "condition": "dialog.attendeeResult.contactsList !=null && count(dialog.attendeeResult.contactsList) > 0",
          "elseActions": [
            {
              "$kind": "Microsoft.TextInput",
              "$designer": {
                "id": "d1fnSD"
              },
              "maxTurnCount": 3,
              "alwaysPrompt": false,
              "allowInterruptions": false,
              "prompt": "${TextInput_Prompt_d1fnSD()}",
              "property": "dialog.NewAttendee"
