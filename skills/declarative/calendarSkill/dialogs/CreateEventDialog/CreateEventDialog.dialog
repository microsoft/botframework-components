        {
          "$kind": "Microsoft.SetProperty",
          "$designer": {
            "id": "yGoNtQ"
          },
          "property": "$slots.end.dateTime",
          "value": "=addSeconds(formatDateTime($slots.start.dateTime), int($dateTimeResolution.value))"
        }
      ]
    },
