                  "$designer": {
                    "id": "Gf8yQO"
                  },
                  "activityProcessed": true,
                  "dialog": "ResolveDateTimeDialog",
                  "options": {
                    "query": "=turn.activity.text"
                  },
                  "resultProperty": "$timeResolution"
                },
                {
                  "$kind": "Microsoft.SetProperties",
                  "$designer": {
                    "id": "d7I2p1"
                  },
                  "assignments": [
                    {
                      "property": "$event.start.dateTime",
                      "value": "=formatTicks(ticks(startOfDay(formatDateTime($dateTimeResolution.value))) + (ticks(formatDateTime($timeResolution.value)) - ticks(startOfDay(formatDateTime($timeResolution.value)))))"
                    },
                    {
                      "property": "$event.end.dateTime",
                      "value": "=addMinutes(formatDateTime($event.start.dateTime), int(coalesce($options.duration, settings.defaultValue.duration)))"
                    }
                  ]
                }
              ]
            },
            {
              "value": "time",
              "actions": [
                {
                  "$kind": "Microsoft.SetProperties",
                  "$designer": {
                    "id": "gu7imz"
                  },
                  "assignments": [
                    {
                      "property": "$event.start.dateTime",
                      "value": "=formatDateTime($dateTimeResolution.value)"
                    },
                    {
                      "property": "$event.end.dateTime",
                      "value": "=addMinutes(formatDateTime($event.start.dateTime), int(coalesce($options.duration, settings.defaultValue.duration)))"
                    }
                  ]
                }
              ]
            },
            {
              "value": "datetime",
              "actions": [
                {
                  "$kind": "Microsoft.SetProperties",
                  "$designer": {
                    "id": "pU8xdl"
                  },
                  "assignments": [
                    {
                      "property": "$event.start.dateTime",
                      "value": "=formatDateTime($dateTimeResolution.value)"
                    },
                    {
                      "property": "$event.end.dateTime",
                      "value": "=addMinutes(formatDateTime($event.start.dateTime), int(coalesce($options.duration, settings.defaultValue.duration)))"
                    }
                  ]
                }
