{
  "$schema": "../app.schema",
  "$kind": "Microsoft.Test.Script",
  "httpRequestMocks": [
    "getEvents.mock"
  ],
  "dialog": {
    "$kind": "Microsoft.AdaptiveDialog",
    "triggers": [
      {
        "$kind": "Microsoft.OnBeginDialog",
        "actions": [
          {
            "$kind": "Microsoft.Graph.Calendar.GetEvents",
            "resultProperty": "dialog.result",
            "token": "='token'",
            "isFutureProperty": "=false",
            "startProperty": "='2020-09-11T09:00:00'",
            "endProperty": "='2020-09-11T17:00:00'",
            "timezoneProperty": "='Pacific Standard Time'"
          },
          {
            "$kind": "Microsoft.Graph.Calendar.SortEvents",
            "resultProperty": "dialog.sortResult",
            "inputProperty": "=dialog.result",
            "timezoneProperty": "='Pacific Standard Time'"
          },
          {
            "$kind": "Microsoft.SendActivity",
            "activity": "${dialog.sortResult[0].events[0].Subject}"
          }
        ]
      }
    ]
  },
  "script": [
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "hello"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "Project Review"
    }
  ]
}
