{
  "$schema": "https://raw.githubusercontent.com/microsoft/botbuilder-samples/main/experimental/generation/TestBot/TestBot/TestBot.schema",
  "$kind": "Microsoft.Test.Script",
  "httpRequestMocks": [
    "getProfile.mock",
    "getWorkingHours.mock",
    "getEventsNotOrganizer.mock",
    "deleteEvent.mock",
    "declineEvent.mock"
  ],
  "userTokenMocks": [
    "nomagiccode.mock"
  ],
  "dialog": "calendarskill",
  "script": [
    {
      "$kind": "Microsoft.Test.UserConversationUpdate",
      "membersAdded": [
        "Bot",
        "User"
      ],
      "membersRemoved": []
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "Hi, Megan Bowen! How can I help you today?"
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "cancel my meeting with lidia"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "I'm searching your calendar, this might take a sec"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "I see you are not the owner of the meeting, would you like to decline the meeting? (1) Yes or (2) No",
      "assertions": [
        "attachments[0].contentType == 'application/vnd.microsoft.card.adaptive'"
      ]
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "yes"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "Alright, Iâ€™ve declined the meeting named Test Meeting.",
      "description": "Response to input 1"
    }
  ]
}
