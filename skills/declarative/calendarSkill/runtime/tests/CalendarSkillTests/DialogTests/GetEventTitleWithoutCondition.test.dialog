    {
      "$kind": "Microsoft.Test.UserSays",
      "text" : "project team meeting"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "I found 1 event for Thursday, December 31st. It‚Äôs at 7:00 AM, Project Team Meeting. The attendees are Lidia Holloway and 4 more. ",
