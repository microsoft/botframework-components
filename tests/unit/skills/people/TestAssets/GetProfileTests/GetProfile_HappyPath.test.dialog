{
  "$schema": "https://raw.githubusercontent.com/microsoft/botbuilder-samples/main/experimental/generation/TestBot/TestBot/TestBot.schema",
  "$kind": "Microsoft.Test.Script",
  "userTokenMocks": [
    "nomagiccode.mock"
  ],
  "dialog": "People",
  "script": [
    {
      "$kind": "Microsoft.Test.UserActivity",
      "activity": {
        "type": "event",
        "name": "SetTestOptions",
        "value": {
          "randomSeed": 0,
          "randomValue": 0
        }
      }
    },
    {
      "$kind": "Microsoft.Test.UserConversationUpdate",
      "membersAdded": [
        "Bot",
        "User"
      ],
      "membersRemoved": []
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "Hi Test User üñê, I can help you search for people in your organization."
    },
    {
      "$kind": "Microsoft.Test.UserSays",
      "text": "tell me more about Thomas Chung"
    },
    {
      "$kind": "Microsoft.Test.AssertReply",
      "text": "Here is what I found for **Thomas Chung**:"
    }
  ]
}
